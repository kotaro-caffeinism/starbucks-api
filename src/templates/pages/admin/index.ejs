<%- include('../../partials/header'); %>
<h2>admin</h2>
<button>ドリンクを追加する</button>
<button>ドリンクを編集する</button>
<button>ドリンクを削除する</button>
<table cellspacing="0">
  <thead>
    <th scope="col">Name</th>
    <th scope="col">Category</th>
    <th scope="col">Shop</th>
    <th scope="col">isSeasonal</th>
  </thead>
  <tbody>
    <% beverages.forEach(function (beverage) { %>
    <tr>
      <td><%= beverage.name %></td>
      <td><%= beverage.category %></td>
      <td><%= beverage.shop %></td>
      <td><%= beverage.isSeasonal %></td>
    </tr>
    <% }); %>
  </tbody>
</table>
<%- include('../../partials/footer'); %>
